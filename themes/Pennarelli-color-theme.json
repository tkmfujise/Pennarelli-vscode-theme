			"name": "List",
			"scope": "markup.list",
			"settings": {
				"foreground": "#C82088"
			}
		},
		{
			"name": "Bold Markup",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#C1AFFF"
			}
		},
		{
			"name": "Italic Markup",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF0000"
			}
