		{
			"name": "JSON Key",
			"scope": "string.json support.type.property-name.json",
			"settings": {
				"foreground": "#e3770a"
			}
		},
		{
			"name": "HTML data-xxx attributes key",
			"scope": "text.html meta.attribute.data-x entity.other.attribute-name.html",
