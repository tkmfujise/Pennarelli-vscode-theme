				"meta.embedded"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#002240"
			}
		},
		{
			"name": "Entity inherited-class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#000000"
			}
		},
		{
			"name": "String embedded-source",
			"scope": "string.quoted source",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "String constant",
			"scope": "string constant",
			"settings": {
				"foreground": "#00AD01"
			}
		},
		{
			"name": "String.regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#00BA62"
			}
		},
		{
			"name": "String variable",
			"scope": "string variable",
			"settings": {
				"foreground": "#40BB00"
			}
		},
		{
			"name": "Support.function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Support Function Macro",
			"scope": "support.function.macro",
			"settings": {
				"foreground": "#CD3B3B"
			}
		},
		{
			"name": "Punctuation Section Embedded",
			"scope": "punctuation.section.embedded",
			"settings": {
				"fontStyle": "",
				"foreground": "#ACAA8C"
			}
		},
		{
			"name": "Support.constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#B50E1B"
			}
		},
		{
			"name": "Exception",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#FF1E00"
			}
		},
		{
			"name": "Doctype/XML Processing",
			"scope": [
				"meta.tag.metadata.doctype entity",
				"meta.tag.metadata.doctype string",
				"meta.tag.metadata.processing.xml",
				"meta.tag.metadata.processing.xml entity",
				"meta.tag.metadata.processing.xml string"
			],
			"settings": {
				"foreground": "#73817D"
			}
		},
		{
			"name": "Meta.tag.A",
			"scope": [
				"meta.tag",
				"meta.tag entity"
			],
			"settings": {
				"foreground": "#1498cc"
			}
		},
		{
			"name": "css#id",
			"scope": "entity.other.attribute-name.id.css",
			"settings": {
				"foreground": "#cc08cc"
			}
		},
		{
			"name": "css.class",
			"scope": "entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#cc9b08"
			}
		},
		{
			"name": "css property-name:",
			"scope": "support.type.property-name.css",
			"settings": {
				"foreground": "#5ed3a8"
			}
		},
		{
			"name": "css property-value;",
