{
	"tokenColors": [
		{
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation - (punctuation.definition.string | punctuation.definition.comment)",
			"settings": {
				"fontStyle": "",
				"foreground": "#3D3C0A"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#e3770a"
			}
		},
		{
			"name": "Boolean",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "",
				"foreground": "#4E7CFF"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"fontStyle": "",
				"foreground": "#EB4A51"
			}
		},
		{
			"name": "Type name",
			"scope": "entity.name.type",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#000000"
			}
		},
		{
			"name": "Type name in definition",
			"scope": "meta.function.definition entity.name.type",
			"settings": {
				"foreground": "#f47628"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#bb23de"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#2020ff"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "",
				"foreground": "#974687"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"fontStyle": "",
				"foreground": "#1E2F32"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#6E5EBE"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.unquoted.heredoc string"
