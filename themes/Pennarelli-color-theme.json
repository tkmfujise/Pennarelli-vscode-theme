		{
			"name": "Other Variable",
			"scope": "variable.other",
			"settings": {
				"fontStyle": "",
				"foreground": "#1E2F32"
			}
		},
		{
			"name": "Other Variable Readwrite",
			"scope": "variable.other.readwrite",
			"settings": {
				"fontStyle": "",
				"foreground": "#20a3ba"
			}
		},
		{
			"name": "Variable Constant",
			"scope": [
				"variable.other.constant",
				"meta.attribute"
